# Mirrors for Linux and BusyBox.

# Linux kernel
LINUX_MIRROR_OFFICIAL := https://www.kernel.org/pub/linux/kernel
LINUX_MIRROR_CDN := https://cdn.kernel.org/pub/linux/kernel
LINUX_MIRROR_TSINGHUA := https://mirrors.tuna.tsinghua.edu.cn/kernel
LINUX_MIRROR_ALIYUN := https://mirrors.aliyun.com/linux-kernel
LINUX_MIRROR_USTC := https://mirrors.ustc.edu.cn/kernel.org/linux/kernel
LINUX_MIRROR_CUSTOME:= @LINUX_MIRROR_CUSTOME@

# BusyBox
BUSYBOX_MIRROR_OFFICIAL := https://busybox.net/downloads
BUSYBOX_MIRROR_CUSTOME:= @BUSYBOX_MIRROR_CUSTOME@

# ÂµClibc
UCLIBC_MIRROR_OFFICIAL := https://uclibc.org/downloads
UCLIBC_MIRROR_CUSTOME:= @UCLIBC_MIRROR_CUSTOME@


LINUX_MIRROR = $(LINUX_MIRROR_@LINUX_MIRROR@)
BUSYBOX_MIRROR = $(BUSYBOX_MIRROR_@BUSYBOX_MIRROR@)
UCLIBC_MIRROR = $(UCLIBC_MIRROR_@UCLIBC_MIRROR@)
